package aula1;

import java.util.Scanner;

public class Ex1 {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		
		String str = in.nextLine();
		
		int contD = 0;
		int contW = 0;
		int contMin = 0;
		int contMai = 0;
		int contL = 0;
		boolean allMin=false;
		boolean allMai=false;
		for(int i=0;i<str.length();i++) {
			char c = str.charAt(i);
			
			if(Character.isDigit(c)) contD++;
			
			if(Character.isLetter(c)) {							//se for letra
				contL++;
				if(Character.toLowerCase(c)==c) contMin++;		//verifica se é minuscula
				if(Character.toUpperCase(c)==c) contMai++;
			}
			if(contMin==contL) allMin=true;
			else allMin=false;
			if(contMai==contL) allMai=true;
			else allMai=false;
		}
		//System.out.printf("a) %i//b) %s//c) %s//d) %i//e) %s", cont, allMin, allMai,);
		System.out.println("a) " + contD);
		System.out.println("b) " + allMin);
		System.out.println("c) " + allMai);
	}

}
