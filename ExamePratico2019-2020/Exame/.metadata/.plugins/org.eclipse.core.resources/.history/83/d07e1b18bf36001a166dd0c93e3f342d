
public class Individual extends Cliente {
	
	private int nTel;

	public Individual(String nome, String nCont, TipoCliente tipo, int nTel) {
		super(nome, nCont, tipo);
		assert (nTel > 100000000 && nTel < 999999999) || nTel == null : "Número de telemóvel inválido";
		this.nTel = nTel;
	}
	
//	public Individual(String nome, String nCont, TipoCliente tipo) {
//		this(nome, nCont, tipo, null)		
//	}

	public Individual(String nome2, String nCont2, TipoCliente tipo2) {
		this(nome, nCont, tipo, )
	}

	public int getnTel() {
		return nTel;
	}

	@Override
	public String toString() {
		return "Individual [nTel=" + nTel + "]";
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = super.hashCode();
		result = prime * result + nTel;
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (!super.equals(obj))
			return false;
		if (getClass() != obj.getClass())
			return false;
		Individual other = (Individual) obj;
		if (nTel != other.nTel)
			return false;
		return true;
	}
	
}
