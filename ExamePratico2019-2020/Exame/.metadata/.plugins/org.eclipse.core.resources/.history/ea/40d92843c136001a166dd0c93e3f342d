import java.util.Set;

public class Banco {
	
	private String nome;
	private Set<Conta> contas;
	
	public Banco(String nome) {
		assert nome.length() > 0 : "Nome do Banco inválido";
		this.nome = nome;
	}

	public String getNome() {
		return nome;
	}

	public Set<Conta> getContas() {
		return contas;
	}

	public String add(Conta c) {
		
	}
	
	public boolean addB(Conta c) {
		return contas.add(c);
	}

}
